<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.blockchain"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/download1"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.blockchain.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.blockchain.Selection"
            android:label="@string/title_activity_selection" >
        </activity>
        <activity
            android:name="com.example.blockchain.RegisterKey"
            android:label="@string/title_activity_register_key" >
        </activity>
        <activity
            android:name="com.example.blockchain.MainPage"
            android:label="@string/title_activity_main_page" >
        </activity>
        <activity
            android:name="com.example.blockchain.Locker"
            android:label="@string/title_activity_locker" >
        </activity>
        <activity
            android:name="com.example.blockchain.AddData"
            android:label="@string/title_activity_add_data" >
        </activity>
        <activity
            android:name="com.example.blockchain.ViewData"
            android:label="@string/title_activity_view_data" >
        </activity>
    </application>

</manifest>
